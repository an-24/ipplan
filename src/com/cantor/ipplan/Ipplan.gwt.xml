<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='ipplan'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.logging.Logging"/>
  <set-property name="gwt.logging.popupHandler" value="DISABLED" />
  <inherits name="com.google.gwt.i18n.I18N"/>
  <extend-property name="locale" values="ru"/>
  <set-property-fallback name="locale" value="ru"/>
  
  <set-property name="user.agent" value="ie8,ie9,gecko1_8,safari,opera"/>
  
  <inherits name="com.cantor.ipplan.theme.blitzer.Blitzer"/>

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.cantor.ipplan.client.UserProfile'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
